(defwidget time_module []    
    (eventbox
        :onclick "eww open dashboardWindow_left --toggle"
        :cursor "pointer"
        (box
        :class "time_module"
        :space-evenly false
        :spacing 20
        :orientation "h"
        (label :text {formattime(EWW_TIME, "%b %d - %I:%M:%S")})
    ))
)