(defwindow wallpaper
  :geometry (geometry
    :width "100%"
    :height "100%"
    :anchor "center"
  )
  
  :windowtype "desktop"
  :stacking "fg"

  (eventbox
    :cursor "pointer"
    :hexpand true
    :vexpand true
    :onclick "notify-send 'Click Detectado'"
    (box
      :class "wallpaperBox"
      :hexpand true
      :vexpand true
      :orientation "v"
      :space-evenly false
      (box
        :hexpand true
        :vexpand true
        :class "wallpaperImage"
      )
    )
  )
)