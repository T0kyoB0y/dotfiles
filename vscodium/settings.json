    "scm.inputFontFamily": "FiraCode Nerd Font",
    "notebook.output.fontFamily": "'FiraCode Nerd Font'",
    "git.openRepositoryInParentFolders": "never",
    "editor.acceptSuggestionOnEnter": "off",
    "editor.inlayHints.enabled": "offUnlessPressed",
    "liveServer.settings.donotShowInfoMsg": true,
    "auto-rename-tag.activationOnLanguage": ["*"],
    "liveServer.settings.donotVerifyTags": true,
    "editor.accessibilitySupport": "off",
    "editor.definitionLinkOpensInPeek": true,
    "java.autobuild.enabled": false,
    "diffEditor.renderSideBySide": false,
    "diffEditor.wordWrap": "on",
    "editor.suggest.preview": true,
    "editor.suggestSelection": "recentlyUsedByPrefix",
    "breadcrumbs.filePath": "off",
    "terminal.integrated.cursorBlinking": true,
    "editor.suggest.localityBonus": true,
    "breadcrumbs.symbolSortOrder": "type",
    "terminal.explorerKind": "external",
    "vscode_custom_css.imports": [
      "file:///home/tokyob0t/Documents/VSCodiumCSS/styles.css",
      "file:///home/tokyob0t/.vscode-oss/extensions/brandonkirbyson.vscode-animations-2.0.1/dist/updateHandler.js"
    ],
    "terminal.external.linuxExec": "kitty",
    "vscode_custom_css.statusbar": false,
    "notebook.compactView": false,
    "debug.showInlineBreakpointCandidates": false,
    "editor.stickyTabStops": true,
    "update.showReleaseNotes": false,
    "update.mode": "none",
    "markdown.preview.breaks": true,
    "markdown-preview-github-styles.colorTheme": "dark",
    "notebook.insertToolbarLocation": "hidden",
    "notebook.globalToolbarShowLabel": "never",
    "editor.parameterHints.cycle": false,
    "editor.parameterHints.enabled": false,
    "debug.onTaskErrors": "abort",
    "animations.Scrolling": "Slide",
    "editor.linkedEditing": true,
    "animations.Focus-Dimming-Mode": "None",
    "animations.Enabled": true,
    "java.foldingRange.enabled": false,
    "animations.Active": "None",
    "breadcrumbs.symbolPath": "off",
    "vsintellicode.features.python.deepLearning": "enabled",
    "gutterpreview.showImagePreviewOnGutter": false,
    "editor.quickSuggestionsDelay": 0,
    "editor.quickSuggestions": {
      "comments": "on",
      "strings": "on"
    },
    "telemetry.telemetryLevel": "off",
    "workbench.layoutControl.enabled": false,
    "window.commandCenter": false,
    "animations.CursorAnimation": true,
    "animations.CursorAnimationOptions": {
      "Color": "#3ddbd9"
    },
    "workbench.startupEditor": "none",
    "editor.fontFamily": "fira-code-nerd, FiraCode Nerd Font, Consolas, 'Courier New', monospace",
    "workbench.reduceMotion": "off",
    "editor.renderWhitespace": "trailing",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "[astro]": {
      "editor.defaultFormatter": "astro-build.astro-vscode"
    },
    "editor.detectIndentation": false,
    "color-highlight.languages": [
        "*"
    ],
    "color-highlight.matchWords": true,
    "colorize.colorized_colors": [
        "BROWSERS_COLORS",
        "HEXA",
        "RGB",
        "HSL"
    ],
    "colorize.enable_search_variables": true,
    "colorize.colorized_variables": [
        "CSS",
        "SASS"
    ],
    "colorize.hide_current_line_decorations": false,
    "[python]": {
        "editor.defaultFormatter": "ms-python.python"
    },
    "html.format.wrapLineLength": 0,
    "[yuck]": {
        "editor.defaultFormatter": "eww-yuck.yuck"
    },
    "workbench.colorTheme": "Another Colorscheme",
    "window.zoomLevel": 1
